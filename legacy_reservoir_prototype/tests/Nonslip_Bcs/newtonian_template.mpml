    <vector_field name="Analytic" rank="1">
      <prescribed>
        <mesh name="PressureMesh_Discontinuous"/>
        <value name="WholeMesh">
          <python>
            <string_value lines="20" type="code" language="python">def val(X,t):
  from numpy import exp,sin,pi
  z=X[1]
  dp=1.0
  mu=1.0e0
  rho=1
  h=1.0
  
  u=0.0
  for k in range(30):
    uk=8.0*h**2/(pi**3*(2*k+1)**3)*sin(pi*(2*k+1)*z/h)
    u+=-(1.0-exp(-mu*pi**2*(2*k+1)**2*t))*dp/(2.0*mu)*uk
  return [u,0.0]</string_value>
          </python>
        </value>
        <output/>
        <stat>
          <include_in_stat/>
        </stat>
        <detectors>
          <exclude_from_detectors/>
        </detectors>
      </prescribed>
    </vector_field>
