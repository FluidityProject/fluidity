                              attribute rank { "0" },
                              attribute name { "Immobile_chemical_src" },
                              element diagnostic {
                                   mesh_choice_pressure,
			                    darcy_impes_internal_algorithm,
			                    darcy_impes_diagnostic_scalar_field,
			                    darcy_impes_recalculation_options?
