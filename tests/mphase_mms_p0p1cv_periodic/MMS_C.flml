              <python>
                <string_value lines="20" type="code" language="python">def val(XX, t):
    from math import sin,cos
    nu = 0.7;
    adv = 1.0;
    beta = 0.0;
    mass = 1.0;
    rho = 1.0;
    x = XX[0];
    y = XX[1];
    alpha = 1.0 - (0.2 + cos(2.0*x)/10.0)
    d_alpha_x = sin(2.0*x)/5.0
    d_alpha_y = 0.0
    Su = adv*alpha*rho*(cos(x)*sin(x)*sin(y)**2 + cos(x)*sin(x)*cos(y)**2) + 2*nu*alpha*sin(x)*cos(y) - nu*d_alpha_x*cos(x)*cos(y) - alpha*sin(2.0*x)/2.0
    Sv = adv*alpha*rho*(sin(x)**2*cos(y)*sin(y) + cos(x)**2*cos(y)*sin(y)) - 2*nu*alpha*cos(x)*sin(y) - nu*d_alpha_x*sin(x)*sin(y) - alpha*sin(2.0*y)/2.0
    return (Su, Sv)</string_value>
