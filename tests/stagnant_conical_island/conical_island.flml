    <detectors>
      <detector_array name="LandslideRamp">
        <number_of_detectors>
          <integer_value rank="0">653</integer_value>
        </number_of_detectors>
        <static/>
        <python>
          <string_value lines="20" type="python">def val(t):
   from numpy import arange, zeros
   from math import sqrt
   offset = 0.00001                            # offset from boundary
   delta = 0.005                              # step size
   Rslope = 4.4124                             # radius of slope section (.ne. island)
   R0 = 2.07                                   # radius of water
   sigma = 0.14854                             # landslide offset
   a = 0.4                                     # landslide length
   X0 = R0 - 3*sigma/sqrt(10) - a*3.0/sqrt(10) # landslide centre
   mx = (1.2+0.27080128015453203)/(0.81240384046359604+3.6) # x slope of slope segment
   c = (3.0-(1.2+0.27080128015453203))         # constant of slope segment
   xcoord = arange(X0-a, Rslope, delta)        # the x coordinates along this slope segment
   coords = []
   for x in xcoord:
      coord = [x, offset, -(mx*x + c) + offset]
      coords.append(coord)
   return coords</string_value>
        </python>
      </detector_array>
      <detector_array name="7Rsym">
        <number_of_detectors>
          <integer_value rank="0">20</integer_value>
        </number_of_detectors>
        <static/>
        <python>
          <string_value lines="20" type="python">def val(t):
   from numpy import pi, sin, cos, tan, arange
   gamma = -86.3*pi/180.               # angle from +ve x axis of detector array
   offset = 0.00001                    # offset from boundary
   delta = 0.005                      # step size
   length = 0.05                       # length of detectors from shoreline (up and down)
   R0 = 2.07                           # radius of water
   ####  SLOPE INFO 73IslandSideLeft ####
   # starting angle of wedge from negative y axis (note that gamma is from positive x axis)
   theta = 0.0
   # angular width of wedge segment
   alpha = pi/14.
   # parameters of lines bounding segment
   m = (-cos(theta+alpha)+cos(theta))/(sin(theta+alpha)-sin(theta))
   c1 = -4.45*(cos(theta)+m*sin(theta))
   c2 = -0.85*(cos(theta)+m*sin(theta))
   # parameters of line along x-axis
   mx = (1.2*m)/(c2-c1)
   cx = 3 + mx*c1/m
   # parameters of line down y-axis
   my = (1.2)/(c1-c2)
   cy = 3 - my*c1 # should equal cx
   ### END OF SLOPE INFO ###
   h = 3.0 - ((4.45 - R0)/3.0)  # depth (+ve) to water surface at start
   X0 = (h - cy)/(mx + my*tan(gamma)) # intersection of coast with detectors
   Y0 = X0*tan(gamma)
   radii = arange(-length, length, delta)
   coords = []
   for r in radii:
     x = X0 + r*cos(gamma)
     y = Y0 + r*sin(gamma)
     z = -(mx*x + my*y + cy) + offset
     coord = [x, y, z]
     coords.append(coord)
   return coords</string_value>
        </python>
      </detector_array>
