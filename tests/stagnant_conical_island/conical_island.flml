        <static/>
        <python>
          <string_value lines="20" type="python">def val(t):
   from numpy import pi, sin, cos, tan, arange
   gamma = -72.9*pi/180.               # angle from +ve x axis of detector array
   offset = 0.00001                    # offset from boundary
   delta = 0.005                      # step size
   length = 0.05                       # length of detectors from shoreline (up and down)
   R0 = 2.07                           # radius of water
   ####  SLOPE INFO 74IslandSideSecondLeft ####
   # starting angle of wedge from negative y axis
   theta = pi/14.
   # angular width of wedge segment  
   alpha = pi/14.
   # parameters of lines bounding segment
   m = (-cos(theta+alpha)+cos(theta))/(sin(theta+alpha)-sin(theta))
   c1 = -4.45*(cos(theta)+m*sin(theta))
