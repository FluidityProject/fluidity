            <string_value type="code" language="python" lines="20">def val(X,t):
  from math import exp
  T0 = 288.15 # K
  L = 0.0065 # K/m
  g = 9.8 # m/s**2
  h = X[0]
  T = T0 - L*h
  gamma = 1.4
  Cv = 718.0
  R_specific = Cv*(gamma-1.0)
  e = T*Cv
  p = 101325.0*(1.0 - ((L*h)/T0))**((g)/(R_specific*L))
  return p</string_value>
