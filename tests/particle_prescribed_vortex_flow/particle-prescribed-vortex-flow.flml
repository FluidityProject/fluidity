      <particle_subgroup name="Circle">
        <initial_position>
          <python>
            <string_value type="code" language="python" lines="20">def val(t):
  from numpy import arange,zeros,reshape,concatenate, cos, pi,sqrt

  a=80
  b=80
  c=437
  x=zeros(c,float)
  y=zeros(c,float)
  k=0
  for i in range(0,b):
    for j in range(0,a):
      if (sqrt(((1.0*i/b)-0.5)**2 + ((1.0*j/a)-0.75)**2)&lt;=0.15):
        x[k] = 1.0*i/b
        y[k] = 1.0*j/b
        k = k + 1

  return reshape(concatenate((x,y)),(2,c)).T</string_value>
          </python>
        </initial_position>
        <attributes>
          <scalar_attribute name="Inside">
            <attribute_value>
              <constant>
                <real_value rank="0">1</real_value>
              </constant>
            </attribute_value>
          </scalar_attribute>
        </attributes>
      </particle_subgroup>
      <particle_subgroup name="Fill">
        <initial_position>
          <python>
            <string_value type="code" language="python" lines="20">def val(t):
  from numpy import arange,zeros,reshape,concatenate, cos, pi,sqrt

  a=80
  b=80
  c=5963
  x=zeros(c,float)
  y=zeros(c,float)
  k=0
  for i in range(0,b):
    for j in range(0,a):
      if (sqrt(((1.0*i/b)-0.5)**2 + ((1.0*j/a)-0.75)**2)&gt;0.15):
        x[k] = 1.0*i/b
        y[k] = 1.0*j/b
        k = k + 1

  return reshape(concatenate((x,y)),(2,c)).T</string_value>
          </python>
        </initial_position>
