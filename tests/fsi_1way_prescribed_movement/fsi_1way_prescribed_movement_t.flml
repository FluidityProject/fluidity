        </scalar_field>
        <vector_field name="SolidVelocity" rank="1">
          <prescribed>
            <python_movement>
              <time_variable_in_python>
                <current_time/>
              </time_variable_in_python>
              <python>
                <string_value lines="20" type="code" language="python">def val(X,t):
  if (t&gt;=0.0):
    movement = (0.05, 0.025)
  else:
    movement = (0.0, 0.0)
  return movement</string_value>
              </python>
            </python_movement>
            <mesh name="frontcylinderSolidCoordinateMesh"/>
            <move_solid_mesh/>
            <output/>
            <stat>
              <include_in_stat/>
            </stat>
            <convergence>
              <include_in_convergence/>
            </convergence>
            <detectors>
              <include_in_detectors/>
            </detectors>
            <steady_state>
              <include_in_steady_state/>
            </steady_state>
          </prescribed>
        </vector_field>
        <fsi_projection>
