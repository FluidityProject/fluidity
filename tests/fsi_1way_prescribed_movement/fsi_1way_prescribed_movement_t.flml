            <continuous>
              <bounded name="Diffuse">
                <boundedness_iterations>
                  <integer_value rank="0">5000</integer_value>
                </boundedness_iterations>
              </bounded>
              <solver>
                <iterative_method name="gmres">
                  <restart>
                    <integer_value rank="0">100</integer_value>
                  </restart>
                </iterative_method>
                <preconditioner name="mg"/>
