            <monitors/>
          </diagnostics>
        </solver>
        <initial_condition name="WholeMesh">
          <python>
            <string_value lines="20" type="code" language="python">def val(X, t):
	if(X[1]&gt;0.5):
		return (0.0,-4000.0)
	else:
		return (0.0,0.0)</string_value>
          </python>
        </initial_condition>
        <boundary_conditions name="no_flow_out_base">
          <surface_ids>
            <integer_value shape="1" rank="1">6</integer_value>
          </surface_ids>
          <type name="dirichlet">
