            <real_value rank="0">1.0E-10</real_value>
          </relative_error>
          <absolute_error>
            <real_value rank="0">0.1e-10</real_value>
          </absolute_error>
          <max_iterations>
            <integer_value rank="0">350</integer_value>
          </max_iterations>
          <never_ignore_solver_failures/>
