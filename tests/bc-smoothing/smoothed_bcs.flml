        <initial_condition name="WholeMesh">
          <python>
            <string_value lines="20" type="code" language="python">def val(X,t):
  uvw = [0,0,0]
  for i in range(3):
    if X[i]&lt;0.499:
      uvw[i] = -1
    else:
      uvw[i] = 1
  return uvw</string_value>
          </python>
        </initial_condition>
        <boundary_conditions name="LeftRight">
