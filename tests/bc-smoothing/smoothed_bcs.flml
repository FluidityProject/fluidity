            <integer_value shape="6" rank="1">1 2 3 4 5 6</integer_value>
          </surface_ids>
          <type name="dirichlet">
            <align_bc_with_cartesian>
              <x_component>
                <python>
                  <string_value lines="20" type="code" language="python">def val(X,t):
  if X[0]&lt;0.499:
    return -1.0
  else:
    return 1.0</string_value>
                </python>
              </x_component>
              <y_component>
                <python>
                  <string_value lines="20" type="code" language="python">def val(X,t):
  if X[1]&lt;0.499:
    return -1.0
  else:
    return 1.0</string_value>
                </python>
                <smoothing>
                  <iterations>
                    <integer_value rank="0">10</integer_value>
                  </iterations>
                </smoothing>
              </y_component>
              <z_component>
