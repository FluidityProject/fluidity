from math import sqrt, pi

vfrac_air = states["Air"].scalar_fields["PhaseVolumeFraction"]
vfrac_tephra = states["Tephra"].scalar_fields["PhaseVolumeFraction"]
velocity_air = states["Air"].vector_fields["P1Velocity"]
velocity_tephra = states["Tephra"].vector_fields["P1Velocity"]
density_tephra = states["Tephra"].scalar_fields["Density"]
density_air = states["Air"].scalar_fields["Density"]
viscosity_air = 1.85e-5
ie_tephra = states["Tephra"].scalar_fields["InternalEnergy"]
ie_air = states["Air"].scalar_fields["InternalEnergy"]

d = 2e-4
k = 0.5
Cv_air = 1406.0
Cv_tephra = 954.0
gamma = 1.33

for i in range(field.node_count):
  # Reynolds number
  magnitude = sqrt((velocity_air.node_val(i)[0] - velocity_tephra.node_val(i)[0])**2 + (velocity_air.node_val(i)[1] - velocity_tephra.node_val(i)[1])**2)
  
  Re = (density_air.node_val(i)*magnitude*d)/viscosity_air
  Pr = Cv_air*gamma*viscosity_air/k
  Nu = (2.0 + 0.16*Re**0.67)
  Q = (6.0*k*vfrac_tephra.node_val(i)*Nu)/(d**2)
  Force = Q*((ie_tephra.node_val(i)/Cv_tephra) - (ie_air.node_val(i)/Cv_air))
  field.set(i, -Force)</string_value>
