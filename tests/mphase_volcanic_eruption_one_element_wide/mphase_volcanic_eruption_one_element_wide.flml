            <string_value lines="20" type="code" language="python">def val(X,t):
  from math import exp
  T0 = 288.15 # K
  L = 0.0065 # K/m
  M = 0.0289644 # kg/mol
  R = 8.31447 # J/(mol K)
  g = 9.8 # m/s**2
  h = X[1] # m
  if(X[1] &lt;= 10000):
    T = T0 - L*h
  else:
    T = T0 - L*10000.0
  gamma = 1.33
  Cv = 1406.0
  e = T*Cv
  p = 101325.0*(1.0 - ((L*h)/T0))**((g*M)/(R*L))
  return p/((gamma-1.0)*e)</string_value>
