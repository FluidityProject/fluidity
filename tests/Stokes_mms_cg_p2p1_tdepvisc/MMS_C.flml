  from numpy import arctanh
  A = 0.25;
  B = 1.9;
  x=XX[0];
  y=XX[1];
  phi_x=(arctanh((x/pi-0.5)*B)/arctanh(0.5*B)+1.0)*pi/2.0
  phi_y=(arctanh((y/pi-0.5)*B)/arctanh(0.5*B)+1.0)*pi/2.0
  T = 1 - phi_y/pi + A*(cos(phi_x)*sin(phi_y))
  return T</string_value>
