        <detectors>
          <include_in_detectors/>
        </detectors>
        <steady_state>
          <include_in_steady_state/>
        </steady_state>
        <consistent_interpolation/>
        <priority>
          <integer_value rank="0">2</integer_value>
        </priority>
        <cap_values/>
      </prognostic>
    </scalar_field>
  </material_phase>
