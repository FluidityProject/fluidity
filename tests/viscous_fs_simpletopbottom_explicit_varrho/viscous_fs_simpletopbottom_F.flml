          <python>
            <string_value type="code" language="python" lines="20">import solution
global dx, F, G
dx = solution.dx
F = solution.nond_F
G = solution.nond_G

def val(X,t):
  if(X[1]&gt;(1.-dx/2.)):
    return F(X[0], t)
  elif(X[1]&lt;dx/2.):
    return G(X[0], t)
  else:
    return 0.0</string_value>
