          <python>
            <string_value lines="20" type="code" language="python">def val(X,t):
  return [t,t]</string_value>
          </python>
        </value>
        <output/>
        <stat>
          <include_in_stat/>
        </stat>
        <detectors>
          <exclude_from_detectors/>
        </detectors>
      </prescribed>
    </vector_field>
  </material_phase>
</fluidity_options>
