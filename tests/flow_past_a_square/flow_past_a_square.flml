        <tensor_field name="Viscosity" rank="2">
          <prescribed>
            <value name="WholeMesh">
              <isotropic>
                <python>
                  <string_value lines="20" type="code" language="python">def val(X,t):
  U = 0.535 # m/s
  H = 0.04 # m
  nu = (U*H)/21400.0
  return nu</string_value>
                </python>
              </isotropic>
            </value>
            <output/>
          </prescribed>
        </tensor_field>
        <vector_field name="Absorption" rank="1">
          <prescribed>
            <value name="WholeMesh">
              <python>
                <string_value lines="20" type="code" language="python">def val(X,t):
  H = 0.04
  x2 = 20*H - H/2
  if(X[0] &gt;= x2):
    return [2.0, 0]
  else:
    return [0, 0]</string_value>
              </python>
            </value>
            <output/>
            <stat>
              <include_in_stat/>
            </stat>
            <detectors>
              <exclude_from_detectors/>
            </detectors>
