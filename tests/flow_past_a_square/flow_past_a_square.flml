              <y_component>
                <constant>
                  <real_value rank="0">0.0</real_value>
                </constant>
              </y_component>
            </align_bc_with_cartesian>
          </type>
        </boundary_conditions>
        <boundary_conditions name="InnerSides">
          <surface_ids>
            <integer_value shape="2" rank="1">66 99</integer_value>
