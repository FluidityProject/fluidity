                <output></output>
                <stat></stat>
                <detectors>
                    <exclude_from_detectors></exclude_from_detectors>
                </detectors>
            </prescribed>
        </scalar_field>
        <scalar_field name="AbsoluteDifference" rank="0">
            <diagnostic field_name_b="NumericalSolution" field_name_a="AnalyticalSolution">
