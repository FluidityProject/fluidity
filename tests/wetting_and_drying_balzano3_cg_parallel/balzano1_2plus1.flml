            <integer_value rank="0">2000</integer_value>
          </max_iterations>
          <never_ignore_solver_failures/>
          <diagnostics>
            <monitors/>
          </diagnostics>
        </solver>
        <boundary_conditions name="open_sinus_wave">
          <surface_ids>
            <integer_value shape="1" rank="1">22</integer_value>
          </surface_ids>
          <type name="dirichlet">
            <apply_weakly/>
            <python>
              <string_value lines="20" type="code" language="python">def val(X, t):
    from math import pi, sin
    g=9.81
    if (t&gt;=43200/4):
       return -2.0*g
    else:
       return -2.0*g*sin(2*pi*t/43200) # 12h period with 2m amplitude</string_value>
            </python>
          </type>
        </boundary_conditions>
        <output>
          <include_nonlinear_field/>
        </output>
        <stat/>
        <convergence>
          <include_in_convergence/>
        </convergence>
        <detectors>
          <exclude_from_detectors/>
        </detectors>
        <steady_state>
          <include_in_steady_state/>
        </steady_state>
        <no_interpolation/>
      </prognostic>
    </scalar_field>
    <scalar_field name="Density" rank="0">
