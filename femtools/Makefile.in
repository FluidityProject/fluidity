.SUFFIXES: .f90 .F90 .cpp .c .o .a

# cancel implicit rule that tries to make .o from .mod with m2c:
%.o: %.mod

.f90.o:
	@echo "        FC $<"
	$(FC) $(FCFLAGS) -c $< || (rm $(LIB) && false)
.F.o:
	@echo "        FC $<"
	$(FC) $(FCFLAGS) -c $< || (rm $(LIB) && false)
.F90.o:
	@echo "        FC $<"
	$(FC) $(FCFLAGS) -c $< || (rm $(LIB) && false)
.c.o:
	@echo "        CC $<"
	$(CC) $(CFLAGS) -c $< || (rm $(LIB) && false)
.cpp.o:
	@echo "        CXX $<"
	$(CXX) $(CXXFLAGS) -c $< || (rm $(LIB) && false)

default: $(OBJS)
	@mkdir -p ../lib
	@$(AR) $(ARFLAGS) $(LIB) $(OBJS)
	cp *.mod ../include/

$(F90LIB): $(F90LIB_OBJS)
	@mkdir -p ../lib
	$(AR) $(ARFLAGS) $(F90LIB).a $(F90LIB_OBJS)
	cp *.mod ../include/

clean:
	rm -f *.o *.d *.mod Reference_count_*.F90 $(F90LIB).a

